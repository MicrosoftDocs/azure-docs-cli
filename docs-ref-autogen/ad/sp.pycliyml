- commandList: []
  htmlId: create-for-rbac
  examples:
  - title: Create role assignments with defaults.
    code: az ad sp create-for-rbac
  - title: Create role assignments with a custom name.
    code: az ad sp create-for-rbac -n "http://my-app"
  - title: Create role assignments at the same time.
    code: az ad sp create-for-rbac -n "http://my-app" --role contributor --scopes /subscriptions/11111111-2222-3333-4444-555555555555/resourceGroups/mygroup /subscriptions/11111111-2222-3333-4444-666666666666/resourceGroups/my-another-group
  - title: Login with a service principal.
    code: az login --service-principal -u <name> -p <password> --tenant <tenant>
  - title: Reset credentials on expiration.
    code: az ad sp reset-credentials --name <name>
  - title: Create role assignments.
    code: az role assignment create --assignee <name> --role Contributor
  - title: Revoke the service principal when done with it.
    code: az ad app delete --id <name>
  parameters:
  - name: --expanded-view
    isRequired: False
    summary: Once created, display more information like subscription and cloud environments.
    description: ''
  - name: --name -n
    isRequired: False
    summary: A display name or an app id uri. Command will generate one if missing.
    description: ''
  - name: --password
    isRequired: False
    summary: The password used to login. If missing, command will generate one.
    description: ''
  - name: --role
    isRequired: False
    summary: Role the service principal has on the resources.
    description: ''
    defaultValue: Contributor
  - name: --scopes
    isRequired: False
    summary: Space separated scopes the service principal's role assignment applies to. Defaults to the root of the current subscription.
    description: ''
  - name: --skip-assignment
    isRequired: False
    summary: Do not create default assignment.
    description: ''
  - name: --years
    isRequired: False
    summary: Years the password will be valid.
    description: ''
    defaultValue: 1
  - name: --debug
    isRequired: False
    summary: Increase logging verbosity to show all debug logs.
    description: ''
  - name: --help -h
    isRequired: False
    summary: Show this help message and exit.
    description: ''
  - name: --output -o
    isRequired: False
    summary: Output format.
    description: ''
    defaultValue: json
    parameterValueGroup: json, jsonc, table, tsv
  - name: --query
    isRequired: False
    summary: JMESPath query string. See <a href="http://jmespath.org/">http://jmespath.org/</a> for more information and examples.
    description: ''
  - name: --verbose
    isRequired: False
    summary: Increase logging verbosity. Use --debug for full debug logs.
    description: ''
